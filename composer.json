{
  "name": "wpgwinnett/wpgwinnett-site",
  "description": "WP Gwinnet Site",
  "license": "GPL-2.0+",
  "type": "project",
  "authors": [
    {
      "name": "Micah Wood",
      "email": "micah@wpscholar.com",
      "homepage": "http://wpscholar.com",
      "role": "Developer"
    }
  ],
  "support": {
    "source": "https://github.com/wpgwinnett/wpgwinnett-site",
    "issues": "https://github.com/wpgwinnett/wpgwinnett-site/issues"
  },
  "repositories": {
    "wpackagist": {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  },
  "require": {
    "johnpbloch/wordpress": "@stable",
    "composer/installers": "@stable"
  },
  "suggest": {
    "wp-cli/wp-cli": "@stable"
  },
  "extra": {
    "wordpress-install-dir": "wp",
    "installer-paths": {
      "content/mu-plugins/{$name}": [
        "type:wordpress-muplugin"
      ],
      "content/plugins/{$name}": [
        "type:wordpress-plugin"
      ],
      "content/themes/{$name}": [
        "type:wordpress-theme"
      ]
    }
  }
}
