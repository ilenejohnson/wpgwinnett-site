	"extra": {
		"wordpress-install-dir": "public_html/cms",
		"installer-paths": {
			"public_html/content/plugins/{$name}": ["type:wordpress-plugin"],
			"public_html/content/themes/{$name}": ["type:wordpress-theme"],
